{
  "comments": {
    "blockComment": [ "(*", "*)" ]
  },
  "brackets": [
    ["{", "}"],
    ["[[", "]]"],
    ["[", "]"],
    ["(", ")"],
    ["<|", "|>"],
    ["\\!\\(", "\\)"],
    ["\\(", "\\)"]
  ],
  "autoClosingPairs": [
    { "open": "{", "close": "}" },
    { "open": "[", "close": "]" },
    { "open": "\\(", "close": "\\)", "notIn": ["string", "comment"] },
    { "open": "(", "close": ")" },
    { "open": "<|", "close": "|>", "notIn": ["string", "comment"] },
    { "open": "\"", "close": "\"", "notIn": ["string"] },
    { "open": "(*", "close": "*", "notIn": ["string"] }
  ],
  "surroundingPairs": [
    ["{", "}"],
    ["[[", "]]"],
    ["[", "]"],
    ["(", ")"],
    ["<|", "|>"],
    ["\"", "\""],
    ["(*", "*)"],
    ["\\!\\(", "\\)"],
    ["\\(", "\\)"]
  ],
  "wordPattern": "([$a-zA-Z]+[$0-9a-zA-Z]*`)*[$a-zA-Z]+[$0-9a-zA-Z]*"
}